Algoritmo Fiesta_mexicana
	Definir N,M Como Entero;
	Escribir "Ingresa m";
	Leer M;
	Escribir "Ingresa n";
	Leer N;
	
		Si m - n > 0 Entonces
			Escribir "Tiene un topper para llevar ", m-n;	
		FinSi
		Si m - n = 0 Entonces
			Escribir "Estamos completos :) ", m-n;
		FinSi
		Si m - n < 0 Entonces
			Escribir "Lo siento no alcanza la comida", m-n;
		FinSi

	
FinAlgoritmo

